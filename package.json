{
    "name": "@proton/analytics",
    "version": "0.1.3",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc --noEmit false --declaration --module es2022 --moduleResolution bundler --outDir dist --rootDir src",
        "test": "vitest run",
        "test:cover": "vitest run --coverage",
        "lint": "eslint --quiet",
        "version:patch": "npm version patch",
        "version:minor": "npm version minor",
        "version:major": "npm version major",
        "publish:prepare": "npm run build && git add dist -f",
        "release:patch": "npm run publish:prepare && npm run version:patch",
        "release:minor": "npm run publish:prepare && npm run version:minor",
        "release:major": "npm run publish:prepare && npm run version:major",
        "push:tags": "git push --follow-tags"
    },
    "publishConfig": {
        "registry": "https://nexus.protontech.ch/repository/web-npm/"
    },
    "devDependencies": {
        "@types/node": "^22.13.10",
        "@vitest/coverage-v8": "^2.1.9",
        "happy-dom": "^15.11.7",
        "typescript": "^5.3.3",
        "vitest": "^2.1.8"
    }
}
